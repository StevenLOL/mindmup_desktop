{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "Free mind mapping on your desktop, based on MindMup.com. Works offline, saves and loads from local drive.",
   "file_handlers": {
      "text": {
         "extensions": [ "mup", "mm" ]
      }
   },
   "icons": {
      "128": "assets/icon-128.png",
      "16": "assets/icon-16.png",
      "48": "assets/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "31",
   "name": "MindMup Desktop - Free Mind Mapping",
   "offline_enabled": true,
   "permissions": [ {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "storage" ],
   "short_name": "MindMup Desktop",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0.14"
}
