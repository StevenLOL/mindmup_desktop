{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "MindMup Desktop - Free Mind Mapping",
  "short_name": "MindMup Desktop",
  "description": "Free mind mapping on your desktop, based on MindMup.com. Works offline, saves and loads from local drive.",
  "version": "0.0.0.14",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ],
  "file_handlers": {
    "text": {
      "extensions": [
          "mup", "mm"
      ]
    }
  },
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "offline_enabled": true
}
